%html
    %body
        %h1 Cache status
        %ul
            %li
                Memory usage:
                = stats.bytes|filesizeformat
            %li
                Keys in cache:
                = stats.curr_items
                of
                = stats.total_items
            %li
                Cache hits:
                = stats.get_hits
                of
                = stats.cmd_get
                :
                %b= hit_rate
            %li
                Cache traffic:
                = stats.bytes_read|filesizeformat
                in,
                = stats.bytes_written|filesizeformat
                out
            %li
                uptime
                = stats.uptime

